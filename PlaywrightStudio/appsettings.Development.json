{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "PlaywrightStudio": "Debug"
    }
  },
  "PlaywrightStudio": {
    "ModelsDirectory": "PageObjectModels",
    "DefaultSchemaPath": "PageObjectModels/page.schema.json",
    "EnableCaching": true,
    "CacheExpiration": "01:00:00",
    "ModelFilePatterns": ["*.json"],
    "SkipSchemaValidation": false,
    "IncludeDebugInfo": true,
    "EmbeddingCache": {
      "Enabled": true,
      "CacheDirectory": "Cache/Embeddings",
      "ExpirationHours": 24,
      "MaxCacheSizeMB": 100
    }
  },
  "Browser": {
    "Headless": false,
    "BrowserType": "Chromium",
    "ViewportWidth": 1920,
    "ViewportHeight": 1080,
    "Timeout": 60000,
    "LaunchOptions": {
      "SlowMo": 100,
      "Devtools": false
    }
  },
  "Ollama": {
    "BaseAddress": "http://192.168.50.213:11434/v1",
    "TimeoutMinutes": 30,
    "Model": "llama3.1:8b",
    "Temperature": 0.8,
    "MaxTokens": 4096,
    "DefaultModelName": "llama3.1:8b",
    "EmbeddingModel": "mxbai-embed-large"
  }
}
